    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "Status: Loading all items...\nPress F1-F6 to buy items"
    statusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    statusLabel.TextSize = 12
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextWrapped = true
    statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    statusLabel.Parent = mainFrame
    
    -- Hotkey Info
    local hotkeyLabel = Instance.new("TextLabel")
    hotkeyLabel.Size = UDim2.new(1, -20, 0, 60)
    hotkeyLabel.Position = UDim2.new(0, 10, 0, 85)
    hotkeyLabel.BackgroundTransparency = 1
    hotkeyLabel.Text = "HOTKEYS:\nF1 - Medium Armor\nF2 - USP\nF3 - Tactical Shotgun\nF4 - Pizza\nF5 - Silencer\nF6 - Donut"
    hotkeyLabel.TextColor3 = Color3.fromRGB(180, 220, 255)
    hotkeyLabel.TextSize = 11
    hotkeyLabel.Font = Enum.Font.Gotham
    hotkeyLabel.TextWrapped = true
    hotkeyLabel.TextXAlignment = Enum.TextXAlignment.Left
    hotkeyLabel.Parent = mainFrame
    
    -- Buttons Container
    local buttonContainer = Instance.new("Frame")
    buttonContainer.Size = UDim2.new(1, -20, 0, 180)
    buttonContainer.Position = UDim2.new(0, 10, 0, 150)
    buttonContainer.BackgroundTransparency = 1
    buttonContainer.Parent = mainFrame
    
    local buttonLayout = Instance.new("UIGridLayout")
    buttonLayout.CellSize = UDim2.new(0.5, -5, 0, 35)
    buttonLayout.CellPadding = UDim2.new(0, 5, 0, 5)
    buttonLayout.StartCorner = Enum.StartCorner.TopLeft
    buttonLayout.Parent = buttonContainer
    
    -- Item Buttons
    local itemButtons = {}
    local itemList = {
        {name = "[Medium Armor]", key = "F1", color = Color3.fromRGB(65, 105, 225)},
        {name = "[USP]", key = "F2", color = Color3.fromRGB(50, 205, 50)},
        {name = "[Tactical Shotgun]", key = "F3", color = Color3.fromRGB(220, 20, 60)},
        {name = "[Pizza]", key = "F4", color = Color3.fromRGB(255, 165, 0)},
        {name = "[Silencer]", key = "F5", color = Color3.fromRGB(138, 43, 226)},
        {name = "[Donut]", key = "F6", color = Color3.fromRGB(255, 105, 180)}
    }
    
    for _, item in ipairs(itemList) do
        local button = Instance.new("TextButton")
        button.BackgroundColor3 = item.color
        button.Text = item.name .. "\n" .. item.key
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.TextSize = 11
        button.Font = Enum.Font.GothamBold
        button.Parent = buttonContainer
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 6)
        corner.Parent = button
        
        itemButtons[item.name] = button
    end
    
    -- Auto-Buy Section
    local autoBuyContainer = Instance.new("Frame")
    autoBuyContainer.Size = UDim2.new(1, -20, 0, 80)
    autoBuyContainer.Position = UDim2.new(0, 10, 0, 335)
    autoBuyContainer.BackgroundTransparency = 1
    autoBuyContainer.Parent = mainFrame
    
    local autoBuyTitle = Instance.new("TextLabel")
    autoBuyTitle.Size = UDim2.new(1, 0, 0, 20)
    autoBuyTitle.Position = UDim2.new(0, 0, 0, 0)
    autoBuyTitle.BackgroundTransparency = 1
    autoBuyTitle.Text = "AUTO BUY SETTINGS"
    autoBuyTitle.TextColor3 = Color3.fromRGB(255, 255, 100)
    autoBuyTitle.TextSize = 13
    autoBuyTitle.Font = Enum.Font.GothamBold
    autoBuyTitle.Parent = autoBuyContainer
    
    local delayLabel = Instance.new("TextLabel")
    delayLabel.Size = UDim2.new(0, 100, 0, 25)
    delayLabel.Position = UDim2.new(0, 0, 0, 25)
    delayLabel.BackgroundTransparency = 1
    delayLabel.Text = "Delay (seconds):"
    delayLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    delayLabel.TextSize = 12
    delayLabel.Font = Enum.Font.Gotham
    delayLabel.TextXAlignment = Enum.TextXAlignment.Left
    delayLabel.Parent = autoBuyContainer
    
    local delayBox = Instance.new("TextBox")
    delayBox.Size = UDim2.new(0, 60, 0, 25)
    delayBox.Position = UDim2.new(0, 105, 0, 25)
    delayBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    delayBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    delayBox.Text = "5"
    delayBox.TextSize = 12
    delayBox.Font = Enum.Font.Gotham
    delayBox.PlaceholderText = "Seconds"
    delayBox.Parent = autoBuyContainer
    
    local delayCorner = Instance.new("UICorner")
    delayCorner.CornerRadius = UDim.new(0, 6)
    delayCorner.Parent = delayBox
    
    local autoBuyButton = Instance.new("TextButton")
    autoBuyButton.Size = UDim2.new(0, 150, 0, 35)
    autoBuyButton.Position = UDim2.new(1, -150, 0, 25)
    autoBuyButton.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
    autoBuyButton.Text = "AUTO BUY ARMOR"
    autoBuyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    autoBuyButton.TextSize = 12
    autoBuyButton.Font = Enum.Font.GothamBold
    autoBuyButton.Parent = autoBuyContainer
    
    local autoBuyCorner = Instance.new("UICorner")
    autoBuyCorner.CornerRadius = UDim.new(0, 6)
    autoBuyCorner.Parent = autoBuyButton
    
    -- Debug Button
    local debugButton = Instance.new("TextButton")
    debugButton.Size = UDim2.new(0, 80, 0, 25)
    debugButton.Position = UDim2.new(0, 170, 0, 25)
    debugButton.BackgroundColor3 = Color3.fromRGB(70, 70, 90)
    debugButton.Text = "RESCAN"
    debugButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    debugButton.TextSize = 11
    debugButton.Font = Enum.Font.GothamBold
    debugButton.Parent = autoBuyContainer
    
    local debugCorner = Instance.new("UICorner")
    debugCorner.CornerRadius = UDim.new(0, 6)
    debugCorner.Parent = debugButton
    
    -- VARIABLEN
    local isAutoBuying = false
    local autoBuyConnection = nil
    local foundItems = {}
    
    -- FUNKTIONEN
    local function findBuyPad(itemName)
        -- Gehe direkt zu workspace.MAP.Pads
        if workspace:FindFirstChild("MAP") then
            local map = workspace.MAP
            if map:FindFirstChild("Pads") then
                local padsFolder = map.Pads
                
                -- Suche das Model mit dem exakten Namen
                for _, model in pairs(padsFolder:GetChildren()) do
                    if model:IsA("Model") and model.Name == itemName then
                        -- Suche den ClickDetector im Model
                        for _, child in pairs(model:GetDescendants()) do
                            if child:IsA("ClickDetector") then
                                return {
                                    Model = model,
                                    ClickDetector = child,
                                    Name = itemName
                                }
                            end
                        end
                    end
                end
            end
        end
        return nil
    end
    
    local function scanAllItems()
        foundItems = {}
        statusLabel.Text = "Status: Scanning for items..."
        
        local allItems = {
            "[Medium Armor]",
            "[USP]",
            "[Tactical Shotgun]",
            "[Pizza]",
            "[Silencer]",
            "[Donut]"
        }
        
        local foundCount = 0
        local foundNames = {}
        
        for _, itemName in ipairs(allItems) do
            local pad = findBuyPad(itemName)
            if pad then
                foundItems[itemName] = pad
                foundCount = foundCount + 1
                table.insert(foundNames, itemName)
                
                -- Update Button
                if itemButtons[itemName] then
                    itemButtons[itemName].BackgroundColor3 = itemList[table.find(allItems, itemName)].color
                    itemButtons[itemName].Text = itemName .. "\n" .. itemList[table.find(allItems, itemName)].key .. " âœ“"
                end
            else
                if itemButtons[itemName] then
                    itemButtons[itemName].BackgroundColor3 = Color3.fromRGB(100, 100, 100)
                    itemButtons[itemName].Text = itemName .. "\nNOT FOUND"
                end
            end
        end
        
        -- Update Status
        if foundCount == #allItems then
            statusLabel.Text = "Status: ALL ITEMS FOUND!\nPress F1-F6 to buy"
        elseif foundCount > 0 then
            statusLabel.Text = "Status: Found " .. foundCount .. "/" .. #allItems .. " items\nFound: " .. table.concat(foundNames, ", ")
        else
            statusLabel.Text = "Status: NO ITEMS FOUND!\nMake sure you're in a game with MAP.Pads"
        end
        
        print("=== SCAN RESULTS ===")
        print("Total items found:", foundCount, "/", #allItems)
        for itemName, pad in pairs(foundItems) do
            print("âœ“", itemName)
        end
        
        return foundItems
    end
    
    local function buyItem(itemName, button)
        if not foundItems[itemName] then
            statusLabel.Text = "Status: " .. itemName .. " not found!"
            if button then
                button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                wait(0.5)
                button.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
            end
            return false
        end
        
        local pad = foundItems[itemName]
        local originalColor = button and button.BackgroundColor3
        
        -- Ã„ndere Button Farbe
        if button then
            button.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
        end
        statusLabel.Text = "Status: Buying " .. itemName .. "..."
        
        -- Versuche zu kaufen
        local success, errorMsg = pcall(function()
            fireclickdetector(pad.ClickDetector)
            return true
        end)
        
        if success then
            statusLabel.Text = "Status: Purchased " .. itemName .. "!"
            if button then
                button.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
            end
            
            -- Erfolgssound
            pcall(function()
                local sound = Instance.new("Sound")
                sound.SoundId = "rbxassetid://4590662766"
                sound.Parent = workspace
                sound:Play()
                game:GetService("Debris"):AddItem(sound, 2)
            end)
        else
            statusLabel.Text = "Status: Purchase failed!"
            if button then
                button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            end
            print("Purchase error:", errorMsg)
        end
        
        -- Reset Button nach 1 Sekunde
        if button then
            wait(1)
            button.BackgroundColor3 = originalColor
        end
        
        if not isAutoBuying then
            statusLabel.Text = "Status: Ready!\nPress F1-F6 to buy items"
        end
        
        return success
    end
    
    -- Auto-Buy Funktionen
    local function startAutoBuy()
        if isAutoBuying then return end
        
        if not foundItems["[Medium Armor]"] then
            statusLabel.Text = "Status: No Medium Armor found for Auto-Buy!"
            return
        end
        
        local delayText = delayBox.Text
        local delaySeconds = tonumber(delayText)
        
        if not delaySeconds or delaySeconds < 1 then
            statusLabel.Text = "Status: Invalid time! (min. 1s)"
            delayBox.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            wait(1)
            delayBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
            return
        end
        
        isAutoBuying = true
        autoBuyButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
        autoBuyButton.Text = "STOP AUTO BUY"
        statusLabel.Text = "Status: Auto-Buying Armor!\nEvery " .. delaySeconds .. " seconds"
        
        -- Auto-Buy Loop
        autoBuyConnection = RunService.Heartbeat:Connect(function()
            if not isAutoBuying then
                autoBuyConnection:Disconnect()
                return
            end
            
            -- Kaufe Medium Armor
            buyItem("[Medium Armor]", itemButtons["[Medium Armor]"])
            
            -- Warte die eingestellte Zeit
            local waitTime = 0
            while waitTime < delaySeconds and isAutoBuying do
                wait(0.1)
                waitTime = waitTime + 0.1
            end
        end)
    end
    
    local function stopAutoBuy()
        if not isAutoBuying then return end
        
        isAutoBuying = false
        if autoBuyConnection then
            autoBuyConnection:Disconnect()
            autoBuyConnection = nil
        end
        
        autoBuyButton.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
        autoBuyButton.Text = "AUTO BUY ARMOR"
        statusLabel.Text = "Status: Auto-Buy stopped!\nPress F1-F6 to buy items"
    end
    
    -- Button Events fÃ¼r alle Items
    for itemName, button in pairs(itemButtons) do
        button.MouseButton1Click:Connect(function()
            if not isAutoBuying then
                buyItem(itemName, button)
            end
        end)
    end
    
    -- Auto-Buy Button
    autoBuyButton.MouseButton1Click:Connect(function()
        if isAutoBuying then
            stopAutoBuy()
        else
            startAutoBuy()
        end
    end)
    
    -- Debug/Rescan Button
    debugButton.MouseButton1Click:Connect(function()
        scanAllItems()
    end)
    
    closeButton.MouseButton1Click:Connect(function()
        stopAutoBuy()
        mainGui:Destroy()
    end)
    
    -- Hotkeys F1-F6
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        
        -- F1 - Medium Armor
        if input.KeyCode == Enum.KeyCode.F1 then
            if not isAutoBuying then
                buyItem("[Medium Armor]", itemButtons["[Medium Armor]"])
            end
        
        -- F2 - USP
        elseif input.KeyCode == Enum.KeyCode.F2 then
            if not isAutoBuying then
                buyItem("[USP]", itemButtons["[USP]"])
            end
        
        -- F3 - Tactical Shotgun
        elseif input.KeyCode == Enum.KeyCode.F3 then
            if not isAutoBuying then
                buyItem("[Tactical Shotgun]", itemButtons["[Tactical Shotgun]"])
            end
        
        -- F4 - Pizza
        elseif input.KeyCode == Enum.KeyCode.F4 then
            if not isAutoBuying then
                buyItem("[Pizza]", itemButtons["[Pizza]"])
            end
        
        -- F5 - Silencer
        elseif input.KeyCode == Enum.KeyCode.F5 then
            if not isAutoBuying then
                buyItem("[Silencer]", itemButtons["[Silencer]"])
            end
        
        -- F6 - Donut
        elseif input.KeyCode == Enum.KeyCode.F6 then
            if not isAutoBuying then
                buyItem("[Donut]", itemButtons["[Donut]"])
            end
        end
    end)
    
    -- Initial Scan
    spawn(function()
        wait(2) -- Warte bis Spiel geladen ist
        scanAllItems()
        
        -- Scan alle 15 Sekunden erneut (falls sich was Ã¤ndert)
        while true do
            wait(100)
            if not isAutoBuying then
                scanAllItems()
            end
        end
    end)
    
    -- Drag Function
    local dragging = false
    local dragStart, startPos
    
    titleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = mainFrame.Position
        end
    end)
    
    titleBar.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - dragStart
            mainFrame.Position = UDim2.new(
                startPos.X.Scale, 
                startPos.X.Offset + delta.X, 
                startPos.Y.Scale, 
                startPos.Y.Offset + delta.Y
            )
        end
    end)
    
    print("ðŸŽ¯ AUTO BUY ALL ITEMS GUI LOADED!")
    print("ðŸ“‹ Items supported:")
    print("   â€¢ [Medium Armor] - F1")
    print("   â€¢ [USP] - F2")
    print("   â€¢ [Tactical Shotgun] - F3")
    print("   â€¢ [Pizza] - F4")
    print("   â€¢ [Silencer] - F5")
    print("   â€¢ [Donut] - F6")
    print("ðŸŽ® Auto-Buy: Buys Medium Armor automatically")
end

-- Continue Button Event
continueButton.MouseButton1Click:Connect(function()
    createMainGUI()
end)

print("ðŸ”§ EXECUTOR SUPPORT INFO LOADED!")
print("ðŸ“ Click 'CONTINUE' to go to main GUI!")

-- EXECUTOR SUPPORT INFO (ENGLISH VERSION) - FIXED RESPAWN BUG
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Support Info GUI (appears first in center)
local supportGui = Instance.new("ScreenGui")
supportGui.Name = "ExecutorSupportInfo"
supportGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local supportFrame = Instance.new("Frame")
supportFrame.Size = UDim2.new(0, 500, 0, 320)
supportFrame.Position = UDim2.new(0.5, -250, 0.5, -160)
supportFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
supportFrame.BorderSizePixel = 0
supportFrame.Parent = supportGui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = supportFrame

-- Title
local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, 0, 0, 50)
titleLabel.Position = UDim2.new(0, 0, 0, 0)
titleLabel.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
titleLabel.Text = "EXECUTOR SUPPORT INFO"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
titleLabel.TextSize = 16
titleLabel.Font = Enum.Font.GothamBold
